{
    "name": "mrsayrax/laravel-tochka",
    "description": "Implementation of API methods Tochka Bank for Laravel.",
    "keywords": [
        "Tochka",
        "API",
        "bank"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey Surin",
            "email": "sss@srx.su"
        }
    ],
    "require": {
        "php": ">=7.0",
        "guzzlehttp/guzzle": ">=6.2.0",
        "illuminate/support": "5.5.x|5.6.x|5.7.x|5.8.x|6.*",
        "illuminate/filesystem": "5.5.x|5.6.x|5.7.x|5.8.x|6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.6",
        "sebastian/version": "^1.0.1"
    },
    "autoload": {
        "psr-4": {
            "MrSayrax\\Tochka\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "MrSayrax\\Tochka\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        },
        "laravel":{
            "providers": [
                "MrSayrax\\Tochka\\DadataServiceProvider"
            ],
            "aliases": {
                "Tochka": "MrSayrax\\Tochka\\Facades\\Tochka"
            }
        }
    }
}
